add_namespace = falange

#Dealing with the Socialists
country_event = {
	id = falange.1
	title = falange.1.t
	desc = falange.1.d

	picture = dealing_with_the_socialists-1
	fire_only_once = yes
	is_triggered_only = yes
	trigger = {
		has_completed_focus = SPR_the_socialists

	}
	mean_time_to_happen = { days = 3 }
	timeout_days = 14
	option = {
		name = falange.1.a
		trigger = {
			
		}
		ai_chance = { factor = 1 }
		add_stability = 0.10
add_political_power = -65
add_war_support = -0.03

		hidden_effect = {
			
		}
	}
	option = {
		name = falange.1.b
		trigger = {
			
		}
		ai_chance = { factor = 1 }
		add_political_power = 10

		hidden_effect = {
			
		}
	}
	option = {
		name = falange.1.c
		trigger = {
			
		}
		ai_chance = { factor = 1 }
		add_manpower = -500
add_stability = 0.02

		hidden_effect = {
			
		}
	}
}

